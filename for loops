name = 'Samaksh'
for i in name:
    print(i)
    if(i=="a"):
        print("This is something special")

colors = ["red","greeen","blue","black"]
for color in colors:
    print(color)
    for i in color:
        print(i)

for k in range(20):
    print(k + 1)
for k in range(2 , 9):
    print(k)
for k in range(1,12,3):
    print(k)

